{
  "expo": {
    "name": "Plusnautic",
    "slug": "plusnautic",
    "version": "0.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/plusnauticIcon.png",
    "scheme": "plusnautic",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/fulllogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": true,
      "bundleIdentifier": "com.plusnautica2.app",
      "buildNumber": "4",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "The app accesses your photos to let you change your profile image.",
        "NSCameraUsageDescription": "The app uses your camera to let you take a picture and set it as your profile image.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone",
        "NSLocationAlwaysAndWhenInUseUsageDescription": " it is not necessary plusnautic"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon_coopharma.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.plusnautica2.app",
      "versionCode": 18,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you change your profile image.",
          "cameraPermission": "The app uses your camera to let you take a picture and set it as your profile image."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7873c811-f59a-4c3a-85c6-4de0584dae63"
      }
    }
  }
}
